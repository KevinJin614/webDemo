<struts>
	<package name="productType" namespace="/productType" extends="base-package">
		<action name="productTypeManage" class="action.ProductTypeAction" method="productTypeManage">
			<result>/forwardBD_productTypeManage</result>
		</action>
		<action name="addProductType" class="action.ProductTypeAction" method="addProductType">
